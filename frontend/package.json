    "e2e": "vue-cli-service e2e",
    "lint": "vue-cli-service lint",
    "e2e:open": "vue-cli-service e2e:open"
  },
  "dependencies": {
    "@google/maps": "^0.4.6",
    "@mdi/font": "^2.8.94",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "css-element-queries": "^1.0.1",
    "custom-event-polyfill": "^1.0.6",
    "diacritics": "^1.3.0",
    "hchs-vue-charts": "^1.2.8",
    "highcharts": "^6.2.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.11",
    "material-design-icons": "^3.0.1",
    "mobile-detect": "^1.4.3",
    "register-service-worker": "^1.5.2",
    "roboto-fontface": "^0.10.0",
    "smoothscroll-polyfill": "^0.4.3",
    "vue": "^2.5.17",
    "vue-analytics": "^5.16.0",
    "vue-body-class": "^2.0.0-beta.2",
    "vue-class-component": "^6.0.0",
    "vue-flip": "^0.2.4",
    "vue-highcharts": "0.0.10",
    "vue-meta": "^1.5.5",
    "vue-moment": "^4.0.0",
    "vue-property-decorator": "^7.2.0",
    "vue-router": "^3.0.1",
    "vue2-google-maps": "^0.9.8",
    "vuetify": "^1.3.1",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.1"
  },
  "devDependencies": {
    "@types/jest": "^23.3.5",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^10.11.7",
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-e2e-cypress": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-plugin-pwa": "^3.0.5",
    "@vue/cli-plugin-typescript": "^3.0.5",
    "@vue/cli-plugin-unit-jest": "^3.0.5",
    "@vue/cli-service": "^3.0.1",
    "@vue/eslint-config-standard": "^3.0.1",
    "@vue/eslint-config-typescript": "^3.0.1",
    "@vue/test-utils": "^1.0.0-beta.24",
    "babel-core": "^7.0.0-0",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "ts-jest": "^23.10.4",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.1.3",
    "vue-slug-input": "^0.1.2",
    "vue-template-compiler": "^2.5.17"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "@vue/standard",
      "@vue/typescript"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "<rootDir>/(tests/unit/**/*.spec.(ts|tsx|js)|**/__tests__/*.(ts|tsx|js))"
    ]
  }
}
